                    <!-- Wrapper Starts -->
            <div id="wrapper">
                <!-- Header Starts -->
                <div id="header">
                    <!-- Logo Starts -->
                    <div id="logo">
                        <h1>Levi9 <font color="#666666"> ProfileBook </font> </h1>
                        <h2> Innovation • Excellence • Deligence </h2>
                    </div>

                    <!-- Logo Ends -->
                </div>
                <!-- Header Ends -->
                <!-- Menu Starts -->
                <div id="menu">
                    <!-- Menu Items Starts -->
                    <ul>
                        @*<li><a href="#"><span>@Html.ActionLink("Home", "Index", "Home")</span></a></li>
                        <li><a href="#"><span>@Html.ActionLink("About Us", "About", "Home")</span></a></li>
